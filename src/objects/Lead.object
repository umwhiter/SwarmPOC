 IF(Lead_Total__c &lt; 10, IMAGE(&apos;/resource/Dollars/dollar_one.png&apos;, &apos;one&apos;),
 IF(Lead_Total__c &lt; 15, IMAGE(&apos;/resource/Dollars/dollar_two.png&apos;, &apos;two&apos;) ,
 IF(Lead_Total__c &lt; 20, IMAGE(&apos;/resource/Dollars/dollar_three.png&apos;, &apos;three&apos;) ,
